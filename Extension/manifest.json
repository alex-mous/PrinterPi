{
  "manifest_version": 2,
  "name": "PrinterPi",
  "version": "3.1",
  "description": "Copy and parse data from eBay's Print a Shipping Label page and PayPal's Acitivty page. Then send it to a local application that forwards the data to a networked thermal printer.",

  "icons": {
    "16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png",
    "256": "logo256.png"
  },

  "browser_action": {
	  "default_popup": "main.html",
	  "default_logo": "logo256.png"
  },

  "background": {
	  "scripts": ["/js/background.js"],
	  "persistent": true
  },

  "permissions": [
    "tabs",
    "nativeMessaging",
    "storage",
	  "*://*/*"
  ],

  "key": ""

}
